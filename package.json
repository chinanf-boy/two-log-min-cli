{
	"name": "two-log-min-cli",
	"version": "1.0.1",
	"description": "ora and debug , just two logger, if debug",
	"license": "MIT",
	"repository": "chinanf-boy/two-log-min-cli",
	"author": {
		"name": "chinanf-boy",
		"email": "865501259@qq.com",
		"url": "http://llever.com"
	},
	"bin": {
		"two-log-min": "cli.js"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"test": "ava -v -s",
		"precommit": "lint-staged",
		"npmUp": "npm version patch && npm publish",
		"git": "git push && git push --tags",
		"pub": "npm run test && npm run npmUp && npm run git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"linters": {
			"*.{js,json,css,md}": [
				"prettier --single-quote --trailing-comma es5 --write",
				"git add"
			]
		},
		"ignore": [
			"**/dist/*.js"
		]
	},
	"files": [
		"cli.js"
	],
	"keywords": [
		"cli-app",
		"cli",
		"log",
		"ora",
		"debug"
	],
	"dependencies": {
		"meow": "^3.7.0",
		"turbocolor": "^2.4.5",
		"two-log-min": "^1.0.6"
	},
	"devDependencies": {
		"ava": "^0.20.0",
		"husky": "1.0.0-rc.13",
		"lint-staged": "^7.0.0"
	}
}
